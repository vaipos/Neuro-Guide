// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model UserResponse {
  userID Int @id @default(autoincrement())
  username String 
  Question1 Int
  Question2 Int
  Question3 Int
  Question4 Int
  Question5 Int
  Question6 Int
  Question7 Int
  Question8 Int
  Diagnosis Int
}

model Treatments {
  id Int @id @default(autoincrement())
  diagnosis String 
  fact1 String
  fact2 String
  fact3 String
  fact4 String
  s1 String @db.VarChar(1000)
  s2 String @db.VarChar(1000)
  s3 String @db.VarChar(1000)
}